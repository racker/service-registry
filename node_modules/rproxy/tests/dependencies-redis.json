{
  "redis": {
    "cmd": ["dev-bin/redis.sh", "tests/conf/redis.conf"],
    "cwd": ["__dirname", ".."],
    "log_file": "test-redis.log",
    "wait_for": "socket",
    "wait_for_options": {
      "host": "127.0.0.1",
      "port": 6379
    },
    "timeout": 15000
  },

  "mock_auth_api": {
    "cmd": ["dev-bin/mock-auth-api-server"],
    "cwd": ["__dirname", ".."],
    "log_file": "test-mock-auth-api-server.log"
  },

  "proxy_server": {
    "cmd": ["bin/server", "-c", "tests/conf/proxy-redis.json"],
    "cwd": ["__dirname", ".."],
    "depends": ["redis"],
    "log_file": "test-proxy.log",
    "wait_for": "socket",
    "wait_for_options": {
      "host": "127.0.0.1",
      "port": 9000
    },
    "timeout": 10000,
    "kill_script": "tests/kill_proxy.sh"
  }
}
